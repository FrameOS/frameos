{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/backend/env/bin/uvicorn",
            "args": [
                "app.fastapi:app",
                "--host",
                "0.0.0.0",
                "--port",
                "8989",
                "--reload"
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "GEVENT_SUPPORT": "True"
            },
            "cwd": "${workspaceFolder}/backend"
        }
    ]
}