{
  "name": "vite-kea-react-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:kea\" \"npm run dev:vite\" -n KEA,VITE -c green,magenta",
    "dev:kea": "kea-typegen watch --write-paths",
    "dev:vite": "vite build --watch",
    "build": "tsc && vite build",
    "prettier": "prettier --write src",
    "preview": "vite preview"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "kea": "^3.0.2",
    "kea-loaders": "^3.0.0",
    "kea-router": "^3.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.4.0",
    "kea-forms": "^3.0.3",
    "kea-test-utils": "^0.2.2",
    "kea-typegen": "^3.1.3",
    "postcss": "^8.4.27",
    "prettier": "^2.7.1",
    "sass": "^1.64.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
