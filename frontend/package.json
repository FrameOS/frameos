{
  "name": "@frameos/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:kea\" \"npm run dev:vite\" -n KEA,VITE -c green,magenta",
    "dev:kea": "kea-typegen watch --write-paths --show-ts-errors",
    "dev:vite": "vite build --watch",
    "build": "kea-typegen write --write-paths && tsc && vite build",
    "prettier": "prettier --write src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@projectstorm/react-diagrams": "^7.0.2",
    "clsx": "^2.0.0",
    "fast-deep-equal": "^3.1.3",
    "kea": "^3.1.5",
    "kea-loaders": "^3.0.0",
    "kea-router": "^3.1.4",
    "kea-subscriptions": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-resizable-panels": "^0.0.55",
    "reactflow": "^11.8.3",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^9.0.3",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.4.0",
    "kea-forms": "^3.0.3",
    "kea-test-utils": "^0.2.2",
    "kea-typegen": "^3.3.0",
    "postcss": "^8.4.27",
    "prettier": "^2.7.1",
    "sass": "^1.64.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "overrides": {
    "react-json-view": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
