{
  "name": "@frameos/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:kea\" \"npm run dev:esbuild\" -n KEA,ESB -c green,magenta",
    "dev:kea": "kea-typegen watch --write-paths --show-ts-errors",
    "dev:esbuild": "npm run clean && node build.mjs --dev",
    "clean": "rm -rf ./dist && mkdir ./dist",
    "build": "npm run build:kea && npm run schema && tsc && npm run build:esbuild",
    "build:kea": "kea-typegen write --write-paths",
    "build:esbuild": "node build.mjs",
    "prettier": "prettier --write src",
    "preview": "vite preview",
    "schema": "npm run schema:build:config_json",
    "schema:build:config_json": "ts-json-schema-generator -f tsconfig.json --path 'src/types.ts' --type 'App' --no-type-check > schema/config_json.json && prettier --write schema/config_json.json",
    "bundle:analyze": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@microlink/react-json-view": "^1.23.0",
    "@monaco-editor/react": "^4.5.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@visx/axis": "^3.5.0",
    "@visx/brush": "^3.6.1",
    "@visx/curve": "^3.3.0",
    "@visx/gradient": "^3.3.0",
    "@visx/group": "^3.3.0",
    "@visx/mock-data": "^3.3.0",
    "@visx/pattern": "^3.3.0",
    "@visx/responsive": "^3.3.0",
    "@visx/scale": "^3.5.0",
    "@visx/shape": "^3.5.0",
    "@visx/vendor": "^3.5.0",
    "clsx": "^2.0.0",
    "copy-to-clipboard": "^3.3.3",
    "fast-deep-equal": "^3.1.3",
    "kea": "^3.1.6",
    "kea-forms": "^3.2.0",
    "kea-loaders": "^3.0.1",
    "kea-router": "^3.1.4",
    "kea-subscriptions": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.0",
    "react-popper": "^2.3.0",
    "react-resizable-panels": "^0.0.55",
    "react-syntax-highlighter": "^15.5.0",
    "react-virtuoso": "^4.6.1",
    "reactflow": "^11.8.3",
    "remark-gfm": "^4.0.0",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-syntax-highlighter": "^15.5.9",
    "@types/uuid": "^9.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.4.0",
    "cors": "^2.8.5",
    "cssnano": "^7.0.1",
    "esbuild": "^0.21.3",
    "esbuild-plugin-less": "^1.3.4",
    "esbuild-sass-plugin": "^3.3.0",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "kea-test-utils": "^0.2.2",
    "kea-typegen": "^3.3.4",
    "postcss": "^8.4.27",
    "postcss-preset-env": "^9.5.13",
    "prettier": "^2.7.1",
    "sass": "^1.64.2",
    "tailwindcss": "^3.4.1",
    "ts-json-schema-generator": "^1.3.0",
    "typescript": "^4.6.4"
  },
  "overrides": {
    "react-json-view": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
